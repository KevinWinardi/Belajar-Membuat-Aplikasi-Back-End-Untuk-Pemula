{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": 0,
        "linebreak-style": 0,
        "eol-last": 0,
        "max-len": 0,
        "no-else-return": 0
    }
}
